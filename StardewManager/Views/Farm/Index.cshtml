@model List<Farm>

<h2>My Farms</h2>

@if (Model.Count ==0) {
    <p>Looks like you haven't added any farms!</p>
    <a asp-controller="Farms" asp-action="Add">Add Farm</a>
}
else {
    <table class="table">
        <tr>
            <th>
                Name
            </th>
        </tr>
        @foreach(var farm in Model)
        {
            <tr>
                <td>@farm.Name</td>
            </tr>
        }
    </table>
}